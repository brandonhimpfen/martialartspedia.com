---
layout: null
permalink: /search.json
---
[
  {% assign all = site.arts | concat: site.topics | concat: site.people | concat: site.sources %}
  {% for p in all %}
  {
    "title": {{ p.title | jsonify }},
    "url": {{ p.url | relative_url | jsonify }},
    "type": {{ p.collection | jsonify }},
    "tags": {{ p.tags | default: empty | jsonify }},
    "edited": {{ p.last_edited | default: "" | jsonify }}
  }{% unless forloop.last %},{% endunless %}
  {% endfor %}
]
